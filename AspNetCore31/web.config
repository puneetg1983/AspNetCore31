<?xml version="1.0" encoding="utf-8"?>
<configuration>

  <location path="." inheritInChildApplications="false">
    <system.webServer>
      <handlers>
        <add name="aspNetCore" path="*" verb="*" modules="AspNetCoreModuleV2" resourceType="Unspecified" />
      </handlers>
      <aspNetCore processPath="dotnet" arguments=".\AspNetCore31.dll" stdoutLogEnabled="false" stdoutLogFile=".\logs\stdout" hostingModel="inprocess" />
    </system.webServer>
  </location>

  <system.webServer>
    <monitoring>
      <triggers>
        <requests count="20" timeInterval="01:00:00" />
      </triggers>
      <actions value="Recycle" minProcessExecutionTime="00:00:00" />
    </monitoring>
    <handlers />
  </system.webServer>

</configuration>